<!-- Se incluye el layout del header -->
<%- include('layouts/header') %>

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Colectivo</h2>
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="/sistema">Home</a>
            </li>
            <li class="breadcrumb-item">
                <a>Colectivo</a>
            </li>
            <li class="breadcrumb-item active">
                <strong>Agregar Colectivo Salud</strong>
            </li>
        </ol>
    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox">
                <div class="ibox-title">
                    <h5>Colectivo Salud</h5>
                </div>
                <div class="ibox-content">
                    <div id="form-health-collective" class="wizard-big wizard-health">
                        <h1><i class="fa fa-window-maximize"></i> Colectivo</h1>
                        <fieldset>
                            <form id="policy-form-health-collective" action="/sistema/add-health-collective" method="POST">
                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Póliza No.</label>
                                                <div class="col-sm-9">
                                                    <input 
                                                        type="text"
                                                        name="numero_colectivo"
                                                        id="numero_colectivo"
                                                        placeholder="Número *" 
                                                        class="form-control"
                                                        oninput="let p=this.selectionStart;this.value=this.value.toUpperCase();this.setSelectionRange(p, p);"
                                                        required
                                                    >
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Tomador</label>
                                                <div class="col-sm-9 field">
                                                    <div class="numeral-selector selector">
                                                        <div class="select-box-wrapper">
                                                            <select name="tipo_id_rif_tomador" id="select_box">
                                                                <option value="V">V</option>
                                                                <option value="E">E</option>
                                                                <option value="J">J</option>
                                                                <option value="G">G</option>
                                                                <option value="I">I</option>
                                                                <option value="F">F</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <input 
                                                        type="text"
                                                        name="id_rif_tomador"
                                                        id="id_rif_tomador"
                                                        onkeyup="pasarNombreTomador2();"
                                                        placeholder="Cédula/Rif Tomador *" 
                                                        class="form-control input-numeral"
                                                        required
                                                    >
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Nombre/Razón</label>
                                                <div class="col-sm-9">
                                                    <input 
                                                        type="text"
                                                        name="nombre_tomador_colectivo"
                                                        id="nombre_tomador_colectivo"
                                                        placeholder="Nombre/Razón Social Tomador *" 
                                                        class="form-control"
                                                        oninput="let p=this.selectionStart;this.value=this.value.toUpperCase();this.setSelectionRange(p, p);"
                                                        required
                                                    >
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Correo Tomador</label>
                                                <div class="col-sm-9">
                                                    <input 
                                                        type="email"
                                                        name="correo_tomador"
                                                        id="correo_tomador"
                                                        placeholder="Correo Tomador *" 
                                                        class="form-control"
                                                        required
                                                    >
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Aseguradora</label>
                                                <div class="col-sm-9">
                                                    <select class="form-control" name="nombre_aseguradora" required>
                                                        <option value="">Seleccione *</option>
                                                        <% insurers.forEach((insurer) => { %>
                                                            <option value="<%= insurer.nombre_aseguradora %>"><%= insurer.nombre_aseguradora %></option>
                                                        <% }); %>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Vigencia desde</label>
                                                <div class="col-sm-9">
                                                    <input 
                                                        type="date"
                                                        name="fecha_desde_colectivo"
                                                        id="fecha_desde_colectivo"
                                                        class="form-control"
                                                        required
                                                    >
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Vigencia hasta</label>
                                                <div class="col-sm-9">
                                                    <input 
                                                        type="date"
                                                        name="fecha_hasta_colectivo"
                                                        id="fecha_hasta_colectivo"
                                                        class="form-control"
                                                        required
                                                    >
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Deducible</label>
                                                <div class="col-sm-9">
                                                    <input 
                                                        type="text"
                                                        name="deducible_colectivo"
                                                        id="deducible_colectivo"
                                                        placeholder="Deducible *" 
                                                        class="form-control currency_format"
                                                        required
                                                    >
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">¿Maternidad?</label>
                                                <div class="col-sm-9">
                                                    <select class="form-control" name="maternidad_colectivo" id="maternidad_colectivo" onchange="showInput()" required>
                                                        <option value="">Seleccione *</option>
                                                        <option value="SÍ">SÍ</option>
                                                        <option value="NO">NO</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group" id="searchinput" style="display:none">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Cobertura / Suma</label>
                                                <div class="col-sm-9">
                                                    <input 
                                                        type="text"
                                                        name="cobertura_suma_asegurada_colectivo"
                                                        id="cobertura_suma_asegurada_colectivo"
                                                        placeholder="Cobertura / Suma Asegurada" 
                                                        class="form-control currency_format"
                                                    >
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Plazos de Espera</label>
                                                <div class="col-sm-9">
                                                    <select class="form-control" name="plazo_espera_colectivo" id="plazo_espera_colectivo" onchange="showInput2()" required>
                                                        <option value="">Seleccione *</option>
                                                        <option value="SÍ">SÍ</option>
                                                        <option value="NO">NO</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group" id="searchinput2" style="display:none">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Detalle Cliente</label>
                                                <div class="col-sm-9">
                                                    <input 
                                                        type="date"
                                                        name="detalle_cliente_colectivo"
                                                        id="detalle_cliente_colectivo" 
                                                        class="form-control"
                                                    >
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Grupo</label>
                                                <div class="col-sm-9">
                                                    <input 
                                                        type="text"
                                                        name="grupo_colectivo"
                                                        id="grupo_colectivo"
                                                        placeholder="Grupo" 
                                                        class="form-control"
                                                        oninput="let p=this.selectionStart;this.value=this.value.toUpperCase();this.setSelectionRange(p, p);"
                                                    >
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Moneda</label>
                                                <div class="col-sm-9">
                                                    <select class="form-control" name="tipo_moneda_colectivo" id="tipo_moneda_colectivo" onchange="currencySignChange()" required>
                                                        <option value="">Seleccione *</option>
                                                        <option value="BOLÍVAR">BOLÍVAR</option>
                                                        <option value="DÓLAR">DÓLAR</option>
                                                        <option value="EUROS">EUROS</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Prima Anual</label>
                                                <div class="col-sm-9">
                                                    <input 
                                                        type="text"
                                                        name="prima_anual_colectivo"
                                                        id="prima_anual_colectivo"
                                                        placeholder="Prima Anual *" 
                                                        class="form-control currency_format"
                                                        required
                                                    >
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Canal</label>
                                                <div class="col-sm-9">
                                                    <select class="form-control" name="tipo_canal_colectivo" id="tipo_canal_colectivo" onchange="showList()" required>
                                                        <option value="">Seleccione *</option>
                                                        <option value="REDES SOCIALES">REDES SOCIALES</option>
                                                        <option value="PÁGINA WEB">PÁGINA WEB</option>
                                                        <option value="ÁREA BY ATINA">ÁREA BY ATINA</option>
                                                        <option value="AGENTE PROPIO">AGENTE PROPIO</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group" id="searchList" style="display:none">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Agentes Propios</label>
                                                <div class="col-sm-9">
                                                    <select class="form-control" name="nombre_agentes_propios" id="nombre_agentes_propios">
                                                        <option value="">Seleccione</option>
                                                        <% ownAgents.forEach((ownAgent) => { %>
                                                            <option value="<%= ownAgent.nombre_agente_propio %> <%= ownAgent.apellido_agente_propio %>"><%= ownAgent.nombre_agente_propio %> <%= ownAgent.apellido_agente_propio %></option>
                                                        <% }); %>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Coordinador Suscripción</label>
                                                <div class="col-sm-9">
                                                    <select class="form-control" name="nombre_ejecutivo_coordinador" required>
                                                        <option value="">Seleccione *</option>
                                                        <% executives.forEach((executive) => { %>
                                                            <% if (executive.departamento_cargo_ejecutivo === 'SUSCRIPCIÓN COLECTIVO') { %>
                                                                <option value="<%= executive.nombre_ejecutivo %> <%= executive.apellido_ejecutivo %>"><%= executive.nombre_ejecutivo %> <%= executive.apellido_ejecutivo %></option>
                                                            <% } %>
                                                        <% }); %>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Ejecutivo de Suscripción</label>
                                                <div class="col-sm-9">
                                                    <select class="form-control" name="nombre_ejecutivo_suscripcion" required>
                                                        <option value="">Seleccione *</option>
                                                        <% executives.forEach((executive) => { %>
                                                            <% if (executive.departamento_cargo_ejecutivo === 'SUSCRIPCIÓN') { %>
                                                                <option value="<%= executive.nombre_ejecutivo %> <%= executive.apellido_ejecutivo %>"><%= executive.nombre_ejecutivo %> <%= executive.apellido_ejecutivo %></option>
                                                            <% } %>
                                                        <% }); %>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Ejecutivo de Siniestros</label>
                                                <div class="col-sm-9">
                                                    <select class="form-control" name="nombre_ejecutivo_siniestros" required>
                                                        <option value="">Seleccione *</option>
                                                        <% executives.forEach((executive) => { %>
                                                            <% if (executive.departamento_cargo_ejecutivo === 'SINIESTROS') { %>
                                                                <option value="<%= executive.nombre_ejecutivo %> <%= executive.apellido_ejecutivo %>"><%= executive.nombre_ejecutivo %> <%= executive.apellido_ejecutivo %></option>
                                                            <% } %>
                                                        <% }); %>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Ejecutivo de Cobranzas</label>
                                                <div class="col-sm-9">
                                                    <select class="form-control" name="nombre_ejecutivo_cobranzas" required>
                                                        <option value="">Seleccione *</option>
                                                        <% executives.forEach((executive) => { %>
                                                            <% if (executive.departamento_cargo_ejecutivo === 'COBRANZAS') { %>
                                                                <option value="<%= executive.nombre_ejecutivo %> <%= executive.apellido_ejecutivo %>"><%= executive.nombre_ejecutivo %> <%= executive.apellido_ejecutivo %></option>
                                                            <% } %>
                                                        <% }); %>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <div class="col-sm-12 col-sm-offset-2">
                                        <div class="card-body d-flex">
                                            <button class="btn btn-primary btn-sm ml-auto" id="btn_health_policy_collective" type="submit">Guardar</button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </fieldset>
                        <h1><i class="fa fa-shield"></i> Beneficiarios</h1>
                        <fieldset>
                            <form id="extra-form-health-collective" action="/sistema/add-health-beneficiary-collective" method="POST" enctype="multipart/form-data">
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-2 col-form-label">Tipo de Movimiento</label>
                                                <div class="col-sm-10">
                                                    <select class="form-control" name="tipo_movimiento_beneficiario" required>
                                                        <option value="">Seleccione *</option>
                                                        <option value="EMISIÓN">EMISIÓN</option>
                                                        <option value="RENOVACIÓN">RENOVACIÓN</option>
                                                        <option value="INCLUSIÓN">INCLUSIÓN</option>
                                                        <option value="EXCLUSIÓN">EXCLUSIÓN</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="custom-file">
                                                <input id="excel" name="excel" type="file" class="custom-file-input">
                                                <label for="xlsx" class="custom-file-label">Elija un archivo...</label>
                                            </div> 
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <div class="col-sm-12 col-sm-offset-2">
                                        <div class="card-body d-flex">
                                            <button class="btn btn-primary btn-sm ml-auto" id="btn_health_extra_collective" type="submit">Guardar</button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </fieldset>
                        <h1><i class="fa fa-file-text-o"></i> Recibo</h1>
                        <fieldset>
                            <form id="receipt-form-health-collective" action="/sistema/add-health-receipt-collective" method="POST">
                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Número Recibo</label>
                                                <div class="col-sm-9">
                                                    <input 
                                                        type="text"
                                                        name="numero_recibo"
                                                        id="numero_recibo"
                                                        placeholder="Número de Recibo *" 
                                                        class="form-control"
                                                        oninput="let p=this.selectionStart;this.value=this.value.toUpperCase();this.setSelectionRange(p, p);"
                                                        required
                                                    >
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Tipo de recibo</label>
                                                <div class="col-sm-9">
                                                    <select class="form-control" name="tipo_recibo" required>
                                                        <option value="">Seleccione *</option>
                                                        <option value="EMISIÓN">EMISIÓN</option>
                                                        <option value="RENOVACIÓN">RENOVACIÓN</option>
                                                        <option value="MODIFICACIÓN">MODIFICACIÓN</option>
                                                        <option value="FACTURACIÓN">FACTURACIÓN</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row" id="radioContainer">
                                                <label class="col-sm-3 col-form-label">Fraccionamiento</label>
                                                <div class="col-sm-9">
                                                    <div>
                                                        <label><input type="checkbox" name="fraccionamiento_boolean_recibo" id="fraccionamiento_boolean_recibo" onclick="premiumCalculationDate()"> 
                                                                ¿Aplica Fraccionamiento?
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div id="active-select-1">
                                            <div class="form-group">
                                                <div class="form-group row"><label class="col-sm-3 col-form-label">Tipo</label>
                                                    <div class="col-sm-9">
                                                        <select class="form-control" name="tipo_fraccionamiento_recibo" id="tipo_fraccionamiento_recibo" onchange="premiumCalculationDate()">
                                                            <option value="">Seleccione</option>
                                                            <option value="MENSUAL">MENSUAL</option>
                                                            <option value="BIMESTRAL">BIMESTRAL</option>
                                                            <option value="TRIMESTRAL">TRIMESTRAL</option>
                                                            <option value="SEMESTRAL">SEMESTRAL</option>
                                                            <option value="ANUAL">ANUAL</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <div class="form-group row"><label class="col-sm-3 col-form-label">Número de Pagos</label>
                                                        <div class="col-sm-9">
                                                            <input 
                                                                type="number"
                                                                step="0.01" 
                                                                name="numero_pago_recibo"
                                                                id="numero_pago_recibo"
                                                                value="0" 
                                                                class="form-control"
                                                                onkeyup="premiumCalculationDate();"
                                                            >
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <% if (collective.length !== 0) { %>
                                            <div class="form-group">
                                                <div class="form-group row"><label class="col-sm-3 col-form-label">Prima</label>
                                                    <div class="col-sm-9">
                                                        <input 
                                                            type="text" 
                                                            name="monto_prima_recibo"
                                                            id="monto_prima_recibo"
                                                            value="<%= primaColectivo %>" 
                                                            class="form-control currency_format_number"
                                                            required
                                                        >
                                                    </div>
                                                </div>
                                            </div>
                                        <% } else { %>
                                            <div class="form-group">
                                                <div class="form-group row"><label class="col-sm-3 col-form-label">Prima</label>
                                                    <div class="col-sm-9">
                                                        <input 
                                                            type="text" 
                                                            name="monto_prima_recibo"
                                                            id="monto_prima_recibo"
                                                            placeholder="Monto Prima *" 
                                                            class="form-control currency_format_number"
                                                            required
                                                        >
                                                    </div>
                                                </div>
                                            </div>
                                        <% } %>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Comisión Asociada</label>
                                                <div class="col-sm-9">
                                                    <input 
                                                        type="text" 
                                                        name="monto_comision_recibo"
                                                        id="monto_comision_recibo"
                                                        placeholder="Comisión Asociada *" 
                                                        class="form-control currency_format_number"
                                                        required
                                                    >
                                                </div>
                                            </div>
                                        </div>
                                        <div id="active-select-2">
                                            <div class="form-group">
                                                <div class="form-group row"><label class="col-sm-3 col-form-label">Vigencia Desde</label>
                                                    <div class="col-sm-9">
                                                        <input 
                                                            type="date"
                                                            name="fecha_desde_recibo"
                                                            id="fecha_desde_recibo" 
                                                            class="form-control"
                                                        >
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <div class="form-group row"><label class="col-sm-3 col-form-label">Vigencia Hasta</label>
                                                    <div class="col-sm-9">
                                                        <input 
                                                            type="date"
                                                            name="fecha_hasta_recibo"
                                                            id="fecha_hasta_recibo"
                                                            class="form-control"
                                                        >
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <div class="form-group row"><label class="col-sm-3 col-form-label">Fecha de Pago</label>
                                                    <div class="col-sm-9">
                                                        <input 
                                                            type="date"
                                                            name="fecha_pago_recibo"
                                                            id="fecha_pago_recibo" 
                                                            class="form-control"
                                                        >
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-group row"><label class="col-sm-3 col-form-label">Método de Pago</label>
                                                <div class="col-sm-9">
                                                    <select class="form-control" name="metodo_pago_recibo" required>
                                                        <option value="">Seleccione *</option>
                                                        <option value="TRANSFERENCIA">TRANSFERENCIA</option>
                                                        <option value="ZELLE">ZELLE</option>
                                                        <option value="EFECTIVO">EFECTIVO</option>
                                                        <option value="TARJETA DE DÉBITO">TARJETA DE DÉBITO</option>
                                                        <option value="TARJETA DE CRÉDITO">TARJETA DE CRÉDITO</option>
                                                        <option value="PAYPAL">PAYPAL</option>
                                                        <option value="DEPOSITO BANCARIO">DEPOSITO BANCARIO</option>
                                                        <option value="FINANCIAMIENTO">FINANCIAMIENTO</option>
                                                        <option value="OTROS">OTROS</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group row">
