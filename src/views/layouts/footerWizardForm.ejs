<script>
    $(document).ready(function(){
        $("#wizard").steps();
        $("#form").steps({
            headerTag: "h1",
            bodyTag: "fieldset",
            titleTemplate:"<span class='text'></span> #title#",
            saveState: true,
            onFinished: (a) => {
                $('#form .steps a:eq(0)').click();
                $('#form').hide();
                window.location='/sistema';
            }
        });
    });
</script>

<% if(typeof alert != 'undefined') { %>
